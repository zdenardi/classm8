{
  "tasks": {
    "dev": "deno run -A npm:vite & deno run -A --config deno.json server:start",
    "build": "deno run -A npm:vite build",
    "server:start": "deno run -A --watch ./api/main.ts",
    "serve": "deno run build && deno run server:start",
    "test": "deno test --allow-all --config deno.test.json"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.6",
    "@oak/oak": "jsr:@oak/oak@^17.2.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
    "@types/react": "npm:@types/react@^19.2.7",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.7.0",
    "react": "npm:react@^19.2.3",
    "react-dom": "npm:react-dom@^19.2.3",
    "react-router-dom": "npm:react-router-dom@^7.11.0",
    "vite": "npm:vite@^6.4.1",
    "@prisma/client": "npm:@prisma/client@^7.2.0",
    "@prisma/client/runtime/client": "npm:@prisma/client@^7.2.0/runtime/client",
    "@prisma/adapter-pg": "npm:@prisma/adapter-pg@^7.2.0",
    "pg": "npm:pg@^8.16.3",
    "@clerk/backend": "npm:@clerk/backend@^1.34.0",
    "axios": "npm:axios@^1.7.9",
    "@/prisma": "./prisma/generated/client.ts",
    "@/db": "./api/db.ts",
    "@/test-utils": "./api/util/test/utils.ts"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "target": "esnext",
    "types": ["react", "react-dom", "@types/react"],
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
